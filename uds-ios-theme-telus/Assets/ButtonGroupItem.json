{
  "appearances": {
    "iconPosition": {
      "description": "Theme styles to be applied to icon links depending on where the icon is in relation to the link text",
      "values": ["left", "right"],
      "type": "state"
    },
    "inactive": {
      "description": "Prevents an interactive component from being interacted with and applies accessibility attributes to indicate to the user that this component cannot be used.",
      "values": [true],
      "type": "state"
    },
    "pressed": {
      "description": "Applies while an interactive component is being physically pressed. Most visible on touchscreen taps; in React Native Web, this is visible on web mouse clicks only if the pressable is held down.",
      "values": [true, false],
      "type": "state"
    },
    "selected": {
      "description": "Applies while an interactive component is the currently selected one in a set of states or components, for example a button in a `ButtonGroup`.",
      "values": [true],
      "type": "state"
    },
    "width": {
      "description": "Available in default, or responsive. Default-width expands based on content. Responsive automatically applies full-width when is displayed on the XS viewport.",
      "type": "variant",
      "values": ["responsive"]
    }
  },
  "rules": [
    {
      "if": {
        "pressed": true
      },
      "tokens": {
        "backgroundColor": {
          "red": 0.40392,
          "green": 0.43137,
          "blue": 0.45098,
          "alpha": 1
        },
        "borderColor": {
          "red": 0.40392,
          "green": 0.43137,
          "blue": 0.45098,
          "alpha": 1
        },
        "borderWidth": 0,
        "color": {
          "red": 1,
          "green": 1,
          "blue": 1,
          "alpha": 1
        }
      }
    },
    {
      "if": {
        "selected": true
      },
      "tokens": {
        "backgroundColor": {
          "red": 0.29412,
          "green": 0.15686,
          "blue": 0.42745,
          "alpha": 1
        },
        "borderWidth": 0,
        "color": {
          "red": 1,
          "green": 1,
          "blue": 1,
          "alpha": 1
        }
      }
    },
    {
      "if": {
        "pressed": true,
        "selected": true
      },
      "tokens": {
        "backgroundColor": {
          "red": 0.24706,
          "green": 0.16471,
          "blue": 0.32941,
          "alpha": 1
        }
      }
    },
    {
      "if": {
        "inactive": true
      },
      "tokens": {
        "backgroundColor": {
          "red": 0.69804,
          "green": 0.72549,
          "blue": 0.74902,
          "alpha": 1
        },
        "borderWidth": 0,
        "color": {
          "red": 1,
          "green": 1,
          "blue": 1,
          "alpha": 1
        }
      }
    },
    {
      "if": {
        "iconPosition": "right"
      },
      "tokens": {
        "iconSpace": 1
      }
    },
    {
      "if": {
        "width": "responsive"
      },
      "tokens": {
        "width": "100%"
      }
    }
  ],
  "tokens": {
    "alignSelf": "flex-start",
